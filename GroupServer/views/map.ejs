<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8' />
    <title>Filter features within map view</title>
    <meta name='viewport' content='initial-scale=1,maximum-scale=1,user-scalable=no' />
    <script src='https://api.tiles.mapbox.com/mapbox-gl-js/v0.44.1/mapbox-gl.js'></script>
    <link href='https://api.tiles.mapbox.com/mapbox-gl-js/v0.44.1/mapbox-gl.css' rel='stylesheet' />
	<script src="../plugins/jQuery/jquery-2.2.3.min.js"></script>
    <style>
        body { margin:0; padding:0; }
        #map { position:absolute; top:0; bottom:0; width:100%; }
    </style>
</head>
<body>

<script src="../map/test_data.js"></script>
<script src="../map/mapbox-gl-regex-query.js"></script>
<link rel="stylesheet" href="../plugins/font-awesome-4.7.0/css/font-awesome.min.css" />
<link rel="stylesheet" href="../plugins/bootstrap-3.3.6/css/bootstrap.min.css" />
<script src="../plugins/bootstrap-3.3.6/js/bootstrap.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD1LCWyuLkzDMvyOYxBIvmjyBJsqLDnPA4&libraries=geometry"></script>



<style>
#map {
    position:absolute;
    left:25%;
    top:0;
    bottom:0;
    width: 75%;
}
.map-overlay {
    position: absolute;
    width: 25%;
    top: 0;
    bottom: 0;
    left: 0;
    font: 12px/20px 'Helvetica Neue', Arial, Helvetica, sans-serif;
    background-color: #fff;
    max-height: 100%;
    overflow: hidden;
}

.map-overlay fieldset {
    display: none;
    background: #ddd;
    border: none;
    padding: 10px;
    margin: 0;
}

.map-overlay input {
    display: block;
    border: none;
    width: 100%;
    border-radius: 3px;
    padding: 10px;
    margin: 0;
}

.map-overlay .listing {
    overflow: auto;
    max-height: 100%;
}

.map-overlay .listing > * {
    display: block;
    padding: 5px 10px;
    margin: 0;
}

.map-overlay .listing a {
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    color: #404;
    text-decoration: none;
}

.map-overlay .listing a:last-child {
    border: none;
}

.map-overlay .listing a:hover {
    background: #f0f0f0;
}
</style>

<div id='map'></div>

<div class='map-overlay'>
    <fieldset>
        <input id='feature-filter' type='text' placeholder='Filter results by name' />
    </fieldset>
    <div id='feature-listing' class='listing'></div>
</div>
<nav id="menu"></nav>
<script>
mapboxgl.accessToken = 'pk.eyJ1Ijoic3ZlbmNoZW4iLCJhIjoiY2ludTBwc2k0MTFkaHRxa2pqZDFrenRxbSJ9.0df0NgCDfQSolkaLSmMIIg';
var map = new mapboxgl.Map({
    container: 'map',
    style: 'mapbox://styles/mapbox/streets-v9',
    center: [ -76.9959369166641,
          38.921059689186336],
    zoom: 10
});

// Holds visible airport features for filtering
var airports = [];

var filterEl = document.getElementById('feature-filter');
var listingEl = document.getElementById('feature-listing');
// Create a popup, but don't add it to the map yet.
var popup = new mapboxgl.Popup({
    closeButton: false
});

map.on('load', function() {
	map.addSource('source_test', {
		 type: 'geojson',
		 data: test_data
	});
    map.addLayer({
        "id": "airport",
        "source": 'source_test',
        "type": "symbol",
        "layout": {
            "icon-image": "{marker-symbol}-15",
            "icon-padding": 0,
            "icon-allow-overlap":true
        },
    });

    map.on('moveend', function() {
       updateList()
    });

    map.on('mouseenter', 'airport', function(e) {
        // Change the cursor style as a UI indicator.
        map.getCanvas().style.cursor = 'pointer';

        // Populate the popup and set its coordinates based on the feature.
        var feature = e.features[0];
        popup.setLngLat(feature.geometry.coordinates)
            //.setHTML(feature.properties.name)
            .setHTML(
				'<p>' + e.features[0].properties.name + '</p>\n' +
				'<img src="data:image/jpeg;base64,">'
				)
            .addTo(map);
    });

    map.on('mouseleave', 'airport', function() {
        map.getCanvas().style.cursor = '';
        popup.remove();
    });
	
	var list = [{
		name: '物品',
		type: 'obj',
		color: 'rgba(255,0,0,1)',
		icon: 'aquarium-11'
	},{
		name: '图片',
		type: 'image',
		color: 'rgba(0,255,0,1)',
		icon: 'alcohol-shop-11'
	},{
		name: '语音',
		type: 'audio',
		color: 'rgba(0,0,255,1)',
		icon: 'art-gallery-15'
	},{
		name: '云笔记',
		type: 'note',
		color: 'rgba(255,255,0,1)',
		icon: 'campsite-15'
	},{
		name: '文件/数据',
		type: 'file',
		color: 'rgba(255,0,255,1)',
		icon: 'college-15'
	}];
	
    for (var i = 0; i < list.length; i++) {
		var id = list[i].type;
		var link = document.createElement('a');
		link.href = '#';
		link.className = 'active';
		link.target = id;
		link.textContent = list[i].name;
		link.href = 'javascript:void(0);';
		
		var layers = document.getElementById('menu');
		layers.appendChild(link);
	}

	$('#menu').on('click', 'a', function(e){
		$(this).toggleClass('active');
		updateFilter();
	});
	
	var delay = (function(){
	  var timer = 0;
	  return function(callback, ms){
		clearTimeout (timer);
		timer = setTimeout(callback, ms);
	  };
	})();
	
	$('#feature-filter').on('keyup', function(e) {
		delay(function(){
		  updateFilter();
		}, 1000 );
    });
	
	
	var updateFilter = function(){
		var sub_filter1 = getCatagoryFilter();
		var sub_filter2 = getNameFilter();
		var filter = ['all'];
		filter.push(sub_filter1);
		filter.push(sub_filter2);
		map.setFilter('airport', filter);
		updateList();
	};
	
	var getCatagoryFilter = function(){
		var filter = ['in', 'type'];
		$('#menu').find('.active').each(function(index, item){
			filter.push($(item).attr('target'));
		});
		return filter
	};
	
	var getNameFilter = function(){
		var regex = new RegExp(normalize($('#feature-filter').val()), "i");
		var list = map.getSource('source_test')._data.features
		.map(function(el){
			return el.properties.name;
		})
		.filter(function(el){
			return el.match(regex);
		});
		
		return ['in', 'name'].concat(list);
	};
	
	var updateList = function(){
		setTimeout(function(){
			var features = map.queryRenderedFeatures({layers:['airport']});
			renderListings(features);
		}, 500);
	};
		
	function renderListings(features) {
		// Clear any existing listings
		listingEl.innerHTML = '';
		if (features.length) {
			features.forEach(function(feature) {
				var prop = feature.properties;
				var item = document.createElement('a');
				//item.href = prop.wikipedia;
				item.target = '_blank';
				item.textContent = prop.name //+ ' (' + prop.abbrev + ')';
				item.addEventListener('mouseover', function() {
					// Highlight corresponding feature on the map
					popup.setLngLat(feature.geometry.coordinates)
						.setHTML('<img src="data:image/jpeg;base64,">')
						.addTo(map);
				});
				listingEl.appendChild(item);
			});

			// Show the filter input
			filterEl.parentNode.style.display = 'block';
		} else {
			var empty = document.createElement('p');
			empty.textContent = 'Drag the map to populate results';
			listingEl.appendChild(empty);

			// Hide the filter input
			filterEl.parentNode.style.display = 'none';

			// remove features filter
			// map.setFilter('airport', ['has', 'name']);
		}
	}

	function normalize(string) {
		return string.trim().toLowerCase();
	}

	function getUniqueFeatures(array, comparatorProperty) {
		var existingFeatureKeys = {};
		// Because features come from tiled vector data, feature geometries may be split
		// or duplicated across tile boundaries and, as a result, features may appear
		// multiple times in query results.
		var uniqueFeatures = array.filter(function(el) {
			if (existingFeatureKeys[el.properties[comparatorProperty]]) {
				return false;
			} else {
				existingFeatureKeys[el.properties[comparatorProperty]] = true;
				return true;
			}
		});

		return uniqueFeatures;
	}
	
	updateList()
	
	
	var pushDataToSource = function(list){
		var data = map.getSource('source_test')._data;
		data.features = data.features.concat(list);
		map.getSource('source_test').setData(data);
	};
	
	var createData = function(data){
		console.log(data);
		var icon_map = {
			'obj': 'mountain',
			'image': 'attraction',
			'note': 'art-gallery',
			'audio': 'beer',
			'file': 'rail-metro',
		};
		return {
		  "type": "Feature",
		  "properties": {
			"name": data.name,
			"marker-color": "#0000ff",
			"marker-symbol": icon_map[data.type],
			"type": data.type
		  },
		  "geometry": {
			"type": "Point",
			"coordinates": data.coordinates
		  }
		}
	};
	
	map.on('contextmenu', function(e){
		popup.setLngLat(e.lngLat)
            .setHTML(
				'<p>添加</p>\n' +
				'<div class="btn btn-primary" data-action-add="obj"><i class="fa fa-object-group"></i>物品</div>\n' +
				'<div class="btn btn-warning" data-action-add="image"><i class="fa fa-image"></i>图片</div>\n' +
				'<div class="btn btn-danger" data-action-add="audio"><i class="fa fa-file-audio-o"></i>语音</div>\n' +
				'<p>\n' +
				'<div class="btn btn-success" data-action-add="note"><i class="fa fa-cloud-upload"></i>云笔记</div>\n' +
				'<div class="btn btn-primary" data-action-add="file"><i class="fa fa-file"></i>文件/数据</div>\n' +
				'</p>'
			)
            .addTo(map);
	});	
	
	$('body').on('click', '[data-action-add]', function(e){
		console.log($(this).attr('data-action-add'));
		var data = {
			type: $(this).attr('data-action-add'),
			name: 'New Item',
			coordinates: [
				popup.getLngLat().lng,
				popup.getLngLat().lat
			]
		};
		pushDataToSource([createData(data)]);
		popup.remove();
	});
	
