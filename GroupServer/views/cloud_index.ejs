			.main-block .left-block {
				width: 200px;
				background-color: rgba(0,0,0,.05);
				
			}
			
			.main-block [data-toggle="left-block"] {
				margin-top: 10px;
				padding: 5px;
				cursor: pointer;
				border-radius: 2px;
			}
			
			.main-block [data-toggle="left-block"]:hover {
