			.folder {
				height: 50px;
				color: rgba(0,0,0,.5);
				background-color: rgba(255,255,255,1);
				margin-bottom: 15px;
				-webkit-box-shadow: 0 4px 5px 0 rgba(0,0,0,0.14), 0 1px 10px 0 rgba(0,0,0,0.12), 0 2px 4px -1px rgba(0,0,0,0.2);
				box-shadow: 0 4px 5px 0 rgba(0,0,0,0.14), 0 1px 10px 0 rgba(0,0,0,0.12), 0 2px 4px -1px rgba(0,0,0,0.2);
				cursor: pointer;
				border-radius: 3px;
			}
			
			.folder:hover, .folder.active {
				background-color: rgba(66, 133, 244, 1);
			}
			
			.folder .icon {
				line-height: 50px;
				font-size: 24px;
				border-radius: 50%;
				padding: 4px 6px;
				margin-left: 10px;
				margin-top: -15px;
			}
			
			.folder:hover .icon, .folder.active .icon{
				background-color: rgba(255, 255, 255, 1);
			}
			
			.folder .name {
				display:inline-block;
				padding-left: 15px;
				width:calc(100% - 50px);
				font-weight: 600;
				overflow: hidden;
				white-space:nowrap;
				text-overflow:ellipsis; 
			}
			
			.folder:hover .name, .folder.active .name {
				color: rgba(255, 255, 255, 1);
			}
			
			.file {
				background-color: rgba(255,255,255,1);
				margin-bottom: 15px;
				-webkit-box-shadow: 0 4px 5px 0 rgba(0,0,0,0.14), 0 1px 10px 0 rgba(0,0,0,0.12), 0 2px 4px -1px rgba(0,0,0,0.2);
				box-shadow: 0 4px 5px 0 rgba(0,0,0,0.14), 0 1px 10px 0 rgba(0,0,0,0.12), 0 2px 4px -1px rgba(0,0,0,0.2);
				cursor: pointer;
				border-radius: 3px;
			}
			
			.file .top {
				height: 150px;
				opacity: 0.3;
				/*
				background-image: url(http://127.0.0.1:3000/dist/img/default6.png);
				background-size: 100%;
				*/
			}
			
			.file:hover .top, .file:active .top {
				opacity: 1;
			}
			
			.file:hover .bot , .file.active .bot {
				background-color: rgba(66, 133, 244, 1);
			}
			
			.file .icon {
				padding: 4px 6px;
				margin-left: 10px;
				margin-top: -15px;
				line-height: 50px;
				font-size: 24px;
				border-radius: 50%;
			}
			
			.file:hover .icon, .file.active .icon{
