	var toggleCollapseBlock = function(target){
		var middle = $('#middle-block');
		
		if(target.hasClass('hide')){
			target.removeClass('hide');
			toggleMiddleBlock(-1);
		}else{
			target.addClass('hide');
			toggleMiddleBlock(1);
		}
	};
	
	var toggleMiddleBlock = function(toggle){
		var middle = $('#middle-block');
		middle.attr('data-width', parseInt(middle.attr('data-width')) + toggle);
	};
	
